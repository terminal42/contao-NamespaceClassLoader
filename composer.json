{
    "name":"terminal42/contao-namespace-class-loader",
    "description":"Namespace class loader for Contao Open Source CMS",
    "keywords":["contao", "classloader"],
    "type":"contao-module",
    "license":"LGPL-3.0+",
    "authors":[
        {
            "name":"terminal42 gmbh",
            "homepage":"http://www.terminal42.ch"
        }
    ],
    "require":{
        "php":">=5.3.2",
        "contao/core":">=3.2,<4",
        "contao-community-alliance/composer-plugin":"2.*"
    },
    "replace": {
        "contao-legacy/_autoload": "self.version"
    },
    "extra":{
        "contao": {
            "sources":{
                "": "system/modules/_autoload"
            }
        }
    }
}
