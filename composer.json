{
    "name":"terminal42/namespaceclassloader",
    "description":"Namespace class loader for Contao Open Source CMS",
    "keywords":["contao", "classloader"],
    "type":"contao-module",
    "license":"LGPL-3.0+",
    "authors":[
        {
            "name":"terminal42 gmbh",
            "homepage":"http://terminal42.ch"
        }
    ],
    "require":{
        "php":">=5.3.2",
        "contao/core":">=2.11",
        "contao-community-alliance/composer-installer":"*"
    },
    "replace": {
        "contao-legacy/namespaceclassloader": "self.version"
    },
    "extra":{
        "contao": {
            "sources":{
                "": "system/modules/_autoload"
            }
        }
    }
}
